{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "apiresult",
  "type": "object",
  "title": "iDoklad API result schema",
  "description": "API result of iDoklad APIv3.",
  "required": [
    "Data",
    "ErrorCode",
    "IsSuccess",
    "Message",
    "StatusCode"
  ],
  "properties": {
    "Data": {},
    "ErrorCode": {
      "$id": "#/properties/ErrorCode",
      "type": "integer",
      "title": "Error Code",
      "description": "Code specifying the error.",
      "default": 0,
      "examples": [
        0
      ]
    },
    "IsSuccess": {
      "$id": "#/properties/IsSuccess",
      "type": "boolean",
      "title": "Is Success",
      "description": "Information about the success of the operation.",
      "default": false,
      "examples": [
        true
      ]
    },
    "Message": {
      "$id": "#/properties/Message",
      "type": "string",
      "title": "Message",
      "description": "Error message in case of error, otherwise empty.",
      "default": "",
      "examples": [
        ""
      ]
    },
    "StatusCode": {
      "$id": "#/properties/StatusCode",
      "type": "integer",
      "title": "Statuscode",
      "description": "HTTP return code.",
      "default": 0,
      "examples": [
        200
      ]
    }
  }
}